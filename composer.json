{
	"name": "madewithlove/elasticsearcher",
	"description": "Wrapper on top of the ElasticSearch PHP SDK which allows easier index/document/query management.",
	"keywords": ["elastic","elasticsearch"],
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"ElasticSearcher\\": ["src"]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ElasticSearcher\\Dummy\\": ["tests/dummy"]
		}
	},
	"require": {
		"elasticsearch/elasticsearch": "^5.1",
		"tightenco/collect": "^5.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2-dev"
		}
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	}
}
